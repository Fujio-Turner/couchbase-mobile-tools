{
    "Database": {
        "long_name": "DatabaseConfiguration",
        "constants": [
            {
                "name": "FullSync",
                "value": false,
                "type": {
                    "id": "boolean",
                    "subset": "system"
                },
                "description": "Full sync is off by default because the performance hit is seldom worth the benefit"
            },
            {
                "name": "MmapEnabled",
                "value": true,
                "type": {
                    "id": "boolean",
                    "subset": "system"
                },
                "description": "Memory mapped database files are enabled by default",
                "only_on": ["csharp", "java"]
            },
            {
                "name": "MmapDisabled",
                "value": false,
                "type": {
                    "id": "boolean",
                    "subset": "system"
                },
                "description": "Memory mapped database files are enabled by default",
                "only_on": ["c"]
            }
        ]
    },
    "LogFile": {
        "long_name": "LogFileConfiguration",
        "constants": [
            {
                "name": "UsePlaintext",
                "value": false,
                "type": {
                    "id": "boolean",
                    "subset": "system"
                },
                "description": "Plaintext is not used, and instead binary encoding is used in log files"
            },
            {
                "name": "MaxSize",
                "value": 524288,
                "type": {
                    "id": "size_t",
                    "subset": "system"
                },
                "description": "512 KiB for the size of a log file"
            },
            {
                "name": "MaxRotateCount",
                "value": 1,
                "type": {
                    "id": "uint",
                    "subset": "system"
                },
                "type_swift": {
                    "id": "int",
                    "subset": "system"
                },
                "type_c": {
                    "id": "uint32_t",
                    "subset": "system"
                },
                "type_objc": {
                    "id": "NSInteger",
                    "subset": "system"
                },
                "type_csharp": {
                    "id": "int",
                    "subset": "system"
                },
                "description": "1 rotated file present (2 total, including the currently active log file)"
            }
        ]
    },
    "FileLogSink": {
        "long_name": "FileLogSink",
        "constants": [
            {
                "name": "UsePlaintext",
                "value": false,
                "type": {
                    "id": "boolean",
                    "subset": "system"
                },
                "description": "Plaintext is not used, and instead binary encoding is used in log files"
            },
            {
                "name": "MaxSize",
                "value": 524288,
                "type": {
                    "id": "size_t",
