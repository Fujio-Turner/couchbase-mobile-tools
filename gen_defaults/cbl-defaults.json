                },
                "description": "512 KiB for the size of a log file"
            },
            {
                "name": "MaxKeptFiles",
                "value": 2,
                "type": {
                    "id": "uint",
                    "subset": "system"
                },
                "type_swift": {
                    "id": "int",
                    "subset": "system"
                },
                "type_c": {
                    "id": "uint32_t",
                    "subset": "system"
                },
                "type_objc": {
                    "id": "NSInteger",
                    "subset": "system"
                },
                "type_csharp": {
